<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="296dp"
            android:layout_height="208dp"
            android:background="@drawable/bg_popup_layout"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="233dp"
                android:layout_height="78dp"
                android:text="부산광역시 해운대구 센텀7로 6(우동, 1466)\n영상후반작업시설 201호"
                android:textSize="14dp"
                android:textColor="#101010"
                android:gravity="center"
                android:lineSpacingExtra="1dp"
                android:letterSpacing="-0.05"
                android:fontFamily="@font/sult_medium"
                android:layout_marginTop="41dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/cancelButton"
                android:layout_width="131dp"
                android:layout_height="49dp"
                android:text="취소"
                android:textSize="18dp"
                android:textColor="#FFFFFF"
                android:fontFamily="@font/sult_semibold"
                android:letterSpacing="-0.05"
                android:background="@drawable/bg_popup_gary_btn"
                android:layout_marginLeft="11dp"
                android:layout_marginBottom="11dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/connectCallButton"
                android:layout_width="131dp"
                android:layout_height="49dp"
                android:text="복사하기"
                android:textSize="18dp"
                android:textColor="#FFFFFF"
                android:fontFamily="@font/sult_semibold"
                android:letterSpacing="-0.05"
                android:background="@drawable/bg_button_circle"
                android:layout_marginRight="11dp"
                android:layout_marginBottom="11dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>