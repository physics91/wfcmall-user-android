<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/notExistCouponImageView"
        android:layout_width="81.23dp"
        android:layout_height="162dp"
        android:src="@drawable/img_not_exist_coupon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/sult_bold"
        android:includeFontPadding="false"
        android:text="보유하신 쿠폰이 없어요"
        android:textColor="@color/mainTextColor"
        android:textSize="16dp"
        app:layout_constraintEnd_toEndOf="@+id/notExistCouponImageView"
        app:layout_constraintStart_toStartOf="@+id/notExistCouponImageView"
        app:layout_constraintTop_toBottomOf="@+id/notExistCouponImageView" />

</androidx.constraintlayout.widget.ConstraintLayout>