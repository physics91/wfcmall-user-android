<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="mainTitle"
            type="String" />

        <variable
            name="subTitle"
            type="String" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/notExistDataImageView"
            android:layout_width="101.98dp"
            android:layout_height="143dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/img_empty_search" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/mainTitleTextView"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="66dp"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/sult_medium"
            android:gravity="center"
            android:text="@{mainTitle}"
            android:textColor="@color/mainTextColor"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/notExistDataImageView"
            tools:text="앗! 조건에 맞는 매장이 없어요.테스트메시지" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>